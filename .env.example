# ===========================================
# StreamDock Environment Configuration
# ===========================================
# Copy this file to .env and fill in your values

# PostgreSQL Database
POSTGRES_USER=streamdock
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=streamdock

# TMDB API (https://www.themoviedb.org/settings/api)
TMDB_API_KEY=your_tmdb_api_key_here

# qBittorrent Credentials
QBIT_USERNAME=admin
QBIT_PASSWORD=adminadmin

# Application Secret (generate with: openssl rand -hex 32)
SECRET_KEY=your_secret_key_here

# Network Access (auto-detected by setup.sh)
# This is set automatically when you run ./setup.sh
SERVER_IP=
